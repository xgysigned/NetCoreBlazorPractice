@using Blazor.Diagrams
@using Blazor.Diagrams.Components.Renderers
@using Blazor.Diagrams.Core.Geometry
@using Blazor.Diagrams.Core.Models
<div class="custom-node @(Node.Selected ? "selected" : "")" style="display: inline-block; margin: 0 10px; vertical-align: top;">
   
    <Card Bordered="false" Title=@($"🎯{Node.Title}") Style="width: 150px">
        <Body>
            流程结束
        </Body>
    </Card>
</div>

@code {
    [Parameter]
    public EndNode Node { get; set; }

    public class EndNode : NodeModel
    {
        public EndNode(string id,Blazor.Diagrams.Core.Geometry.Point? position = null) : base(id,position)
        {
            Size = new Size(200, 150);
            AddPort(PortAlignment.Left);
            
        }

        public string Title { get; set; } = "结束";
    }
}